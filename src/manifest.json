{
  "manifest_version": 2,
  "name": "JIRA Task Board Helper",
  "description": "Calculating each assignee's estimate statistic / story points.",
  "version": "0.0.1",
  "permissions": [
    "https://*.atlassian.net/secure/RapidBoard.jspa*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.atlassian.net/secure/RapidBoard.jspa*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ]
}
